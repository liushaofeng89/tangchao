<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="tk.shaofeng.tangchao.model.ServiceChangeHistoryModel" table="servicechangehistory">
        <id name="id" column="ID" type="java.lang.Integer">
        	<generator class="increment"></generator>
        </id>
        <property name="serviceId" column="ServiceID" type="java.lang.Integer"></property>
        <property name="price" column="Price" type="java.lang.Float"></property>
        <property name="lastModifyTime" column="ModifyTime" type="java.util.Date"></property>
    </class>
</hibernate-mapping>
